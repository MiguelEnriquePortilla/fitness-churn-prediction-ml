    "# =============================================================================\n",
    "# TRAIN-TEST SPLIT AND DATA SCALING\n",
    "# =============================================================================\n",
    "\n",
    "print(\"üîÑ TRAIN-TEST SPLIT AND SCALING\")\n",
    "print(\"=\" * 35)\n",
    "\n",
    "# Strategic train-test split with stratification\n",
    "X_train, X_test, y_train, y_test = train_test_split(\n",
    "    X, y, \n",
    "    test_size=0.25,           # 75% train, 25% test\n",
    "    random_state=42,          # Reproducible results\n",
    "    stratify=y                # Maintain churn proportion in both sets\n",
    ")\n",
    "\n",
    "print(f\"üìä SPLIT CONFIGURATION:\")\n",
    "print(f\"   ‚Ä¢ Training set: {X_train.shape[0]:,} customers ({X_train.shape[0]/len(df)*100:.1f}%)\")\n",
    "print(f\"   ‚Ä¢ Test set: {X_test.shape[0]:,} customers ({X_test.shape[0]/len(df)*100:.1f}%)\")\n",
    "print(f\"   ‚Ä¢ Features: {X_train.shape[1]}\")\n",
    "\n",
    "# Verify stratification worked\n",
    "train_churn_rate = y_train.mean() * 100\n",
    "test_churn_rate = y_test.mean() * 100\n",
    "print(f\"\\n‚öñÔ∏è STRATIFICATION VERIFICATION:\")\n",
    "print(f\"   ‚Ä¢ Training churn rate: {train_churn_rate:.1f}%\")\n",
    "print(f\"   ‚Ä¢ Test churn rate: {test_churn_rate:.1f}%\")\n",
    "print(f\"   ‚Ä¢ Difference: {abs(train_churn_rate - test_churn_rate):.1f}pp\")\n",
    "\n",
    "# Feature scaling for Logistic Regression\n",
    "scaler = StandardScaler()\n",
    "X_train_scaled = scaler.fit_transform(X_train)\n",
    "X_test_scaled = scaler.transform(X_test)\n",
    "\n",
    "print(f\"\\nüéØ FEATURE SCALING COMPLETED:\")\n",
    "print(f\"   ‚Ä¢ Scaler: StandardScaler (mean=0, std=1)\")\n",
    "print(f\"   ‚Ä¢ Training data scaled: {X_train_scaled.shape}\")\n",
    "print(f\"   ‚Ä¢ Test data scaled: {X_test_scaled.shape}\")\n",
    "print(f\"   ‚Ä¢ No data leakage: Scaler fitted only on training data\")\n",
    "\n",
    "# Verify scaling\n",
    "print(f\"\\nüìä SCALING VERIFICATION (First Feature):\")\n",
    "print(f\"   ‚Ä¢ Original mean: {X_train.iloc[:, 0].mean():.3f}\")\n",
    "print(f\"   ‚Ä¢ Scaled mean: {X_train_scaled[:, 0].mean():.3f}\")\n",
    "print(f\"   ‚Ä¢ Original std: {X_train.iloc[:, 0].std():.3f}\")\n",
    "print(f\"   ‚Ä¢ Scaled std: {X_train_scaled[:, 0].std():.3f}\")\n",
    "\n",
    "print(\"\\n‚úÖ Data preparation completed - Ready for model training!\")"
   ]

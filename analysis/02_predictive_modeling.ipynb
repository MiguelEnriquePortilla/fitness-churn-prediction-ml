    "# =============================================================================\n",
    "# DATA LOADING AND PREPROCESSING\n",
    "# =============================================================================\n",
    "\n",
    "# Load the dataset\n",
    "df = pd.read_csv('../datasets/gym_churn_us.csv')\n",
    "\n",
    "print(\"üìä DATA LOADING AND PREPROCESSING\")\n",
    "print(\"=\" * 40)\n",
    "print(f\"‚úÖ Dataset loaded: {df.shape[0]:,} customers √ó {df.shape[1]} features\")\n",
    "\n",
    "# Display dataset structure\n",
    "print(\"\\nüîç DATASET STRUCTURE:\")\n",
    "print(f\"   ‚Ä¢ Customers: {df.shape[0]:,}\")\n",
    "print(f\"   ‚Ä¢ Features: {df.shape[1]}\")\n",
    "print(f\"   ‚Ä¢ Target variable: Churn (Binary: 0=Retained, 1=Churned)\")\n",
    "\n",
    "# Feature and target separation\n",
    "# Remove target variable from features\n",
    "feature_columns = [col for col in df.columns if col != 'Churn']\n",
    "X = df[feature_columns]\n",
    "y = df['Churn']\n",
    "\n",
    "print(f\"\\nüìã FEATURE ENGINEERING:\")\n",
    "print(f\"   ‚Ä¢ Features (X): {X.shape[1]} variables\")\n",
    "print(f\"   ‚Ä¢ Target (y): Churn prediction\")\n",
    "print(f\"   ‚Ä¢ Feature names: {list(X.columns)}\")\n",
    "\n",
    "# Check data types and ensure all are numeric\n",
    "print(f\"\\nüî¢ DATA TYPES VALIDATION:\")\n",
    "print(f\"   ‚Ä¢ All features numeric: {X.dtypes.apply(lambda x: x in ['int64', 'float64']).all()}\")\n",
    "print(f\"   ‚Ä¢ Target variable type: {y.dtype}\")\n",
    "print(f\"   ‚Ä¢ Missing values: {X.isnull().sum().sum()}\")\n",
    "\n",
    "# Target variable distribution\n",
    "churn_distribution = y.value_counts(normalize=True) * 100\n",
    "print(f\"\\nüéØ TARGET VARIABLE DISTRIBUTION:\")\n",
    "print(f\"   ‚Ä¢ Retained customers (0): {churn_distribution[0]:.1f}%\")\n",
    "print(f\"   ‚Ä¢ Churned customers (1): {churn_distribution[1]:.1f}%\")\n",
    "print(f\"   ‚Ä¢ Class balance: {'Balanced' if 30 <= churn_distribution[1] <= 70 else 'Slightly Imbalanced'}\")\n",
    "\n",
    "print(\"\\n‚úÖ Data preprocessing completed - Ready for model training!\")"
   ]

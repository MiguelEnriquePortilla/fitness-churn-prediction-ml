    "print(f\"   ‚Ä¢ F1-Score: {log_metrics['f1_score']:.4f} ({log_metrics['f1_score']*100:.1f}%)\")\n",
    "print(f\"   ‚Ä¢ ROC-AUC: {log_metrics['roc_auc']:.4f}\")\n",
    "\n",
    "# Overfitting check\n",
    "overfitting_gap = log_metrics['train_accuracy'] - log_metrics['test_accuracy']\n",
    "print(f\"\\nüîç OVERFITTING ANALYSIS:\")\n",

    "# =============================================================================\n",
    "# MODEL 1: LOGISTIC REGRESSION\n",
    "# =============================================================================\n",
    "\n",
    "print(\"üìä MODEL 1: LOGISTIC REGRESSION\")\n",
    "print(\"=\" * 35)\n",
    "\n",
    "# Initialize and train Logistic Regression\n",
    "log_reg = LogisticRegression(\n",
    "    random_state=42,\n",
